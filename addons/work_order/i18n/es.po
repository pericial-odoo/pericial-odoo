#. module: work_order
#: model:ir.model.fields,help:work_order.field_work_order__state
msgid "* The 'Quotation' status is used when a user is encoding a new                    and unconfirmed work order.\n"
"* The 'Confirmed' status is used when a user confirms the work                         order.\n"
"* The 'In process' status is used to indicate that the work                          order is being done.\n"
"* The 'To be Invoiced' status is used to generate the invoice                         before or after the work order is done.\n"
"* The 'Finished' status is set when the work order is                         completed.\n"
"* The 'Cancelled' status is used when the user cancels the                         work order."
msgstr "* El estado de \"Presupuesto\" es usado cuando un usuario está configurando un nuevo parte de trabajo sin confirmar.\n"
"* El estado \"Confirmado\" es usado cuando un usuario confirma el parte de trabajo.\n"
"* El estado \"En proceso\" es usado para indicar que el parte de trabajo se está llevando a cabo.\n"
"* El estado \"Para facturar\" es usado para generar la factura antes o después de que el parte de trabajo esté hecho.\n"
"* El estado \"Finalizado\" se establece cuando el parte de trabajo esta completado.\n"
"* El estado \"Cancelado\" es usado cuando el usuario cancela el parte de trabajo."


#. module: work_order
#: model:mail.template,body_html:work_order.mail_template_work_order_done
msgid "<?xml version=\"1.0\"?>\n"
"<data><p>Dear ${object.partner_id.name},</p>\n"
"            <p>\n"
"            Here is your work order done: ${doc_name} <strong>${object.name}</strong>\n"
"            % if object.origin:\n"
"            (with reference: ${object.origin} )\n"
"            % endif\n"
"            % if object.invoice_method != 'none':\n"
"            amounting in <strong>${format_amount(object.amount_total, object.pricelist_id.currency_id)}.</strong>\n"
"            % endif\n"
"            </p>\n"
"            <p>Thank you.</p>\n"
"          </data>"
msgstr "<?xml version=\"1.0\"?>\n"
"<data><p>Estimado ${object.partner_id.name},</p>\n"
"            <p>\n"
"            Este es su parte de trabajo finalizado: ${doc_name} <strong>${object.name}</strong>\n"
"            % if object.origin:\n"
"            (with reference: ${object.origin} )\n"
"            % endif\n"
"            % if object.invoice_method != 'none':\n"
"            con un total de <strong>${format_amount(object.amount_total, object.pricelist_id.currency_id)}.</strong>\n"
"            % endif\n"
"            </p>\n"
"            <p>Gracias.</p>\n"
"          </data>"

#. module: work_order
#: model:mail.template,body_html:work_order.mail_template_work_order_quotation
msgid "<?xml version=\"1.0\"?>\n"
"<data><p>Dear ${object.partner_id.name}</p>\n"
"                <p>\n"
"                Here is your work order ${doc_name} <strong>${object.name}</strong>\n"
"                % if object.origin:\n"
"                (with reference: ${object.origin} )\n"
"                % endif\n"
"                % if object.invoice_method != 'none':\n"
"                amounting in <strong>${format_amount(object.amount_total, object.pricelist_id.currency_id)}.</strong>\n"
"                % endif\n"
"                </p>\n"
"                <p>You can reply to this email if you have any questions.</p>\n"
"                <p>Thank you,</p>\n"
"            </data>"
msgstr "<?xml version=\"1.0\"?>\n"
"<data><p>Estimado ${object.partner_id.name}</p>\n"
"                <p>\n"
"                Este es su parte de trabajo ${doc_name} <strong>${object.name}</strong>\n"
"                % if object.origin:\n"
"                (con referencia: ${object.origin} )\n"
"                % endif\n"
"                % if object.invoice_method != 'none':\n"
"                con un total de <strong>${format_amount(object.amount_total, object.pricelist_id.currency_id)}.</strong>\n"
"                % endif\n"
"                </p>\n"
"                <p>Puede contestar a este mail si tiene alguna pregunta.</p>\n"
"                <p>Gracias,</p>\n"
"            </data>"

